<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.patientportal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".FeedbackActivity"></activity>
        <activity android:name=".MyProfileActivity" />
        <activity android:name=".SupervisingDoctorActivity" />
        <activity android:name=".ViewReportActivity" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".DiagnosesActivity" />
        <activity android:name=".MedicalHistoryActivity" />
        <activity android:name=".PastHistoryActivity" />
        <activity android:name=".PrescriptionActivity" />
        <activity android:name=".ComplaintActivity" />
        <activity android:name=".DashActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>